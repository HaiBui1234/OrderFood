<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".allFragment.adminFragment.AdminHomeFragment">
   <SearchView
       android:id="@+id/id_SearchAdmin"
       android:layout_width="match_parent"
       android:layout_height="50dp"
       android:layout_margin="4sp"
       android:layout_alignParentTop="true"
       android:layout_alignParentStart="true"
       android:layout_alignParentEnd="true"
       android:background="@drawable/border_ed"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/id_recyAdminHome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="4sp"
        android:layout_below="@+id/id_SearchAdmin" />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fl_AddFood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="Add"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="5sp"
        android:src="@drawable/ic_action_add"/>
</RelativeLayout>